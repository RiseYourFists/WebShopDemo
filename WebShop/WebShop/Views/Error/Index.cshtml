@{
    ViewData["Title"] = "Error";
}
@model GenericErrorViewModel;

<section id="error-page" class="text-danger">
    <div class="icon fas fa-exclamation-triangle"></div>
    @{
        if (Model.StatusCode.HasValue)
        {
            <h1>@Model.StatusCode</h1>
        }
    }
    <strong>@Model.Error</strong>
</section>
